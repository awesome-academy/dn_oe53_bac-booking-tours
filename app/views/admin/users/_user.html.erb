<tr>
  <td><%= user.id %></td>
  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.phone_number %></td>
  <td>
    <%= link_to admin_user_path(user), remote: :true, method: :delete do %>
       <%= link_to "<i class='fas fa-trash-alt'></i>".html_safe, admin_user_path(user), method: :delete, data: { confirm: "Are you sure?" } %>
    <% end %>
  </td>
</tr>
